package day4;
import java.util.Scanner;
public class countadd {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int start=sc.nextInt();
		int end=sc.nextInt();
		int addcount=0;
		for (int i=start;i<=end;i++)
		{
			if(i%2!=0)
			{
				addcount++;
			}
			System.out.println(addcount);
		}
	}
}
